{
    "presets": [
        [
            "@babel/preset-env",
            {
                "targets": {
                    "browsers": [
                        "last 2 versions",
                        "ie >= 7"
                    ],
                    "node": "8.11.3"
                }
            }   
        ]
    ],
    "plugins": [
        [
            "@babel/transform-runtime",
            {
                "helpers": false
            }
        ],
        "@babel/plugin-syntax-dynamic-import"
    ]
}